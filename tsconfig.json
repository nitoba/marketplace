{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "strict": true,
    "paths": {
      "@components/*": ["src/components/*"],
      "@utils/*": ["src/utils/*"],
      "@screens/*": ["screens/*"],
      "@services/*": ["services/*"],
      "@interfaces/*": ["interfaces/*"],
      "@navigation/*": ["navigation/*"],
      "@constants/*": ["constants/*"],
      "@assets/*": ["assets/*"],
      "@hooks/*": ["src/hooks/*"],
      "@stores/*": ["stores/*"],
      "@api/*": ["api/*"],
      "@atoms/*": ["atoms/*"],
      "@theme/*": ["src/theme/*"]
    }
  }
}
